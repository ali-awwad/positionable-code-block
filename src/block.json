{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "ali-awwad/positionable-code-block",
    "title": "Positionable Code Block",
    "category": "text",
    "icon": "media-code",
    "description": "Add custom code to your site using the Positionable Code block. such as Google Analytics, Google Tag Manager scripts, custom Javascript or CSS, or any other custom code you want to add to your site.",
    "keywords": [ "custom", "code", "html", "css", "javascript", "analytics" ],
    "version": "1.0.3",
    "textdomain": "positionable-code-block",
    "attributes": {
        "location": {
            "enum": [ "head", "body", "footer" ],
            "default": "head"
        },
        "priority": {
            "type": "number",
            "default": 10
        }
    },
    "supports": {
        "align": true
    },
    
    "editorScript": "file:./index.js",
    "render": "file:./render.php"
}